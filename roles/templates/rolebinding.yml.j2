apiVersion: rbac.authorization.k8s.io/v1
kind: RoleBinding
metadata:
  name: "{{ project.name }}"
  namespace: "{{ project.namespace }}"
subjects:
- kind: ServiceAccount
  name: "{{ project.name }}"
  namespace: "{{ project.namespace }}"
roleRef:
  kind: Role
  name: "{{ project.name }}"
  apiGroup: rbac.authorization.k8s.io